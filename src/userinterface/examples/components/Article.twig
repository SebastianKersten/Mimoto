{% extends "examples/components/examplebase.twig" %}

{% block content %}
    <h1>Example 1 - Article</h1>
    <i>An article presented in the primary article layout</i><br>
    <br>
    <br>
    <div class="example_article" {{ Aimless.realtime()|raw }}>
        <i><span {{ Aimless.realtime('lede')|raw }}>{{ Aimless.data('lede') }}</span></i><br><br>
        <b><span {{ Aimless.realtime('title')|raw }}>{{ Aimless.data('title') }}</span></b><br><br>
        <div {{ Aimless.realtime('body')|raw }}>{{ Aimless.data('body') }}</div>
    </div>
{% endblock %}
