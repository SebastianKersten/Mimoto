{% extends "examples/components/examplebase.twig" %}

{% block content %}
    <h1>Example - Project with subprojects</h1>
    <i>This project contains a collection of subprojects and every subproject is presented with the same subproject template.</i><br>
    <br>
    <div class="example_project" {{ Aimless.realtime()|raw }}>

        <b><span {{ Aimless.realtime('name')|raw }}>{{ Aimless.render('name') }}</span></b>

        <span {{ Aimless.realtime('projectManager')|raw }}>{{ Aimless.render('projectManager')|raw }}</span>

        <br><br>
        <div {{ Aimless.realtime('description')|raw }}>{{ Aimless.render('description') }}</div>
        <br>
        <div class="example_project_subprojects" {{ Aimless.realtime('subprojects')|raw }}>
            {{ Aimless.render('subprojects')|raw }}
        </div>
    </div>
{% endblock %}
