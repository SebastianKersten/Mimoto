{% extends "examples/components/examplebase.twig" %}

{% block content %}
    <h1>Author</h1>
    <i>A name and the person's avatar</i><br>
    <br>
    <br>
    <div class="example_article" {{ Mimoto.realtime() }}>
        <i><span {{ Mimoto.realtime('name') }}>{{ Mimoto.data('name') }}</span></i><br><br>
        <img class="example_avatar" src="{{ Mimoto.data('avatar') }}" {{ Mimoto.realtime('avatar') }}>
    </div>
{% endblock %}
