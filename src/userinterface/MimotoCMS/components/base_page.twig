<html>
<head class="MimotoCMS">
    <title>Mimoto.CMS</title>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="stylesheet" type="text/css" href="/mimoto/mimoto.cms.css" />

</head>
<body class="MimotoCMS">
    {% include "MimotoCMS/components/svg.twig" %}

    <div class="MimotoCMS layer_application">

        {% include "MimotoCMS/components/interface/Header/Header.twig" %}
        {% include "MimotoCMS/components/interface/Menu/Menu.twig" %}

        <section class="MimotoCMS_Pagecontent">
            {{ Mimoto.component('content') }}
        </section>

    </div>

    {% include "MimotoCMS/components/interface/Popup/Popup.twig" %}

    <script type="text/javascript" src="/mimoto/mimoto.js"></script>

    <script>
        MimotoX.autoloadCSS = false;
        MimotoX.debug = true;
    </script>

