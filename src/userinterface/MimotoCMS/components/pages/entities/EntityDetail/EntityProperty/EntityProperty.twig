<li class="MimotoCMS_pages_entities_EntityDetail-EntityProperty" {{ Mimoto.realtime() }}>

    <div class="MimotoCMS_pages_entities_EntityDetail-EntityProperty__data">
        <div class="MimotoCMS_pages_entities_EntityDetail-EntityProperty__name" {{ Mimoto.realtime('name') }}>
             {{ Mimoto.data('name') }}
        </div>
        <div class="MimotoCMS_pages_entities_EntityDetail-EntityProperty__type">
            {% if Mimoto.data('subtype') == 'image' or Mimoto.data('subtype') == 'video' or Mimoto.data('subtype') == 'audio' or Mimoto.data('subtype') == 'file' %}
                <div {{ Mimoto.realtime('subtype') }}>{{ Mimoto.data('subtype') }}</div>
            {% else %}
                <div {{ Mimoto.realtime('type') }}>{{ Mimoto.data('type') }}</div>
            {% endif %}
        </div>
        <div class="MimotoCMS_pages_entities_EntityDetail-EntityProperty__propertysettingscontainer" {{ Mimoto.realtime('settings', 'MimotoCMS_entities_EntityDetail-EntityPropertySetting') }}>
            {{ Mimoto.data('settings', 'MimotoCMS_entities_EntityDetail-EntityPropertySetting', { customValues: { propertySubtype:Mimoto.data('subtype') }}) }}
        </div>
    </div>
    <div class="MimotoCMS_pages_entities_EntityDetail-EntityProperty__menu">
        {{ Mimoto.module('MimotoCMS_modules_Button', { label:'Delete', types:['small', 'alert'], directive:Mimoto.remove() }) }}
        {{ Mimoto.module('MimotoCMS_modules_Button', { label:'Edit', types:['small', 'secondary'], directive:Mimoto.edit('_Mimoto_coreform__entityProperty') }) }}
    </div>
    <div class="MimotoCMS_pages_entities_EntityDetail-EntityProperty__menu-handle">
        <div class="MimotoCMS_ListItemModule-handle js-sortable-draghandle">
            <div class="MimotoCMS_ListItemModule-handle-dot"></div>
            <div class="MimotoCMS_ListItemModule-handle-dot"></div>
            <div class="MimotoCMS_ListItemModule-handle-dot"></div>
            <div class="MimotoCMS_ListItemModule-handle-dot"></div>
            <div class="MimotoCMS_ListItemModule-handle-dot"></div>
            <div class="MimotoCMS_ListItemModule-handle-dot"></div>
            <div class="MimotoCMS_ListItemModule-handle-dot"></div>
            <div class="MimotoCMS_ListItemModule-handle-dot"></div>
            <div class="MimotoCMS_ListItemModule-handle-dot"></div>
            <div class="MimotoCMS_ListItemModule-handle-dot"></div>
        </div>
    </div>
</li>
