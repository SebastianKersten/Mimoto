<div class="MimotoCMS_pages_layouts_LayoutDetail-LayoutContainer" {{ Aimless.realtime() }}>

    <div class="MimotoCMS_pages_layouts_LayoutDetail-LayoutContainer__data">
        <div class="MimotoCMS_pages_layouts_LayoutDetail-LayoutContainer__name">
            <div {{ Aimless.realtime('name') }} >{{ Aimless.render('name') }}</div>
        </div>
    </div>
    <div class="MimotoCMS_pages_layouts_LayoutDetail-LayoutContainer__menu">
        {{ Aimless.module('Mimoto.CMS.ButtonModule', { label:'Edit', types:['small', 'secondary'], onclick:"Mimoto.CMS.layoutContainerEdit('" ~ Aimless.meta('id') ~ "');" }) }}
        {{ Aimless.module('Mimoto.CMS.ButtonModule', { label:'Delete', types:['small', 'alert'], onclick:"Mimoto.CMS.layoutContainerDelete('" ~ Aimless.meta('id') ~ "');" }) }}
    </div>
</div>
