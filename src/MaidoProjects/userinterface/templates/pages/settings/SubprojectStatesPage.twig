<div class="page_title">Subproject states</div>

<div id="simplelist" class="simplelist">
    {% for simplelist_item_data in simplelist_data %}
        {% include 'components/data/simplelist_item.twig' with { 'changeMethod':changeMethod, 'id':simplelist_item_data.getId, 'label':simplelist_item_data.getName, 'livescreenid__component':'subprojectstate.'~simplelist_item_data.getId, 'livescreenid__component_label':'subprojectstate.'~simplelist_item_data.getId~'.name' } %}
    {% endfor %}
</div>
<div class="button" onclick="Maido.settings.newSubprojectState();">Add new subproject state +</div>
<script>Maido.notifications.connectPage('subprojectstates');</script>